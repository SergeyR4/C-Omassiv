#include <iostream> // Подключение библиотеки для ввода-вывода
using namespace std; // Использование пространства имён std для краткости

int main() // Основная функция программы
{
    const int ROW = 3; // Определение константы для количества строк
    const int COL = 4; // Определение константы для количества столбцов

    int arr[ROW][COL]{}; // Инициализация двумерного массива целых чисел нулями

    cout << arr[1][1] << endl; // Вывод значения элемента (1,1) массива (по умолчанию 0)

    arr[2][0] = 34; // Присвоение значения 34 элементу (2,0) массива

    cout << arr[2][0] << endl; // Вывод значения элемента (2,0) массива (теперь 34)

    arr[1][1] = 11; // Присвоение значения 11 элементу (1,1) массива
   
    arr[1][2] = 20; // Присвоение значения 20 элементу (1,2) массива
    cout << arr[1][2] << endl; // Вывод значения элемента (1,2) массива (теперь 20)
    cout << arr[1][1] << endl; // Вывод значения элемента (1,1) массива (теперь 11)
}
